{
    "name": "mihaibucse/universe-ingester",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "symfony/config": "^4.2",
        "ext-json": "*",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\" : "src/App"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTests\\": "tests/AppTests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    }
}
